# Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

target 'Preto' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Preto
pod 'Alamofire'
pod 'SDWebImage', '~>3.8'
pod 'Firebase/Auth'
pod 'Firebase/Core'
pod 'Firebase/Messaging'
pod 'FacebookCore'
pod 'FacebookLogin'
pod 'GoogleMaps'

  target 'PretoTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'PretoUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
